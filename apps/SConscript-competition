Import('serialEnv')
Import('parallelEnv')
# TODO(mcj) add nativeEnv as in pbbs to facilitate simulator-free debugging

apps = [#"encoder",
        "BFS",
        "BC",
        "BellmanFord",
        "CF",
        "Components",
        "Radii",
        "PageRank",
        "PageRankDelta",
        "BFSCC",
        "BFS-Bitvector",
        "KCore",
        "MIS",
        "Triangle",
        ]

envs = [serialEnv, parallelEnv]

for env in envs:
    for app in apps:
        env.Program(target=app, source=[app + '.C'])
